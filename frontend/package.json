{
  "name": "vite-react-boilerplate",
  "author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RicardoValdovinos/vite-react-boilerplate"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "git init && npx husky init && npx playwright install && shx rm .husky/pre-commit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
    "storybook:build": "storybook build",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@antv/g6": "^5.0.43",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^6.4.4",
    "@mui/lab": "6.0.0-beta.27",
    "@mui/material": "^6.4.4",
    "@mui/system": "^6.4.3",
    "@mui/x-charts": "^7.27.0",
    "@mui/x-data-grid": "^7.27.0",
    "@mui/x-data-grid-pro": "^7.27.0",
    "@mui/x-date-pickers": "^7.27.0",
    "@mui/x-tree-view": "^7.26.0",
    "@neo4j-nvl/react": "^0.3.6",
    "@nivo/bar": "^0.87.0",
    "@nivo/core": "^0.87.0",
    "@nivo/line": "^0.87.0",
    "@nivo/pie": "^0.87.0",
    "@react-spring/web": "^9.7.5",
    "@tanstack/react-query": "^5.66.3",
    "@tanstack/react-router": "^1.105.0",
    "@tanstack/react-table": "^8.21.2",
    "button": "link:@/components/ui/button",
    "card": "link:@/components/ui/card",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.0.3",
    "i18next-http-backend": "^2.7.3",
    "neo4j-driver": "^5.28.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@faker-js/faker": "^8.4.1",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/devtools": "^4.3.3",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
    "@storybook/addon-links": "^8.5.6",
    "@storybook/addon-themes": "^8.5.6",
    "@storybook/blocks": "^8.5.6",
    "@storybook/react": "^8.5.6",
    "@storybook/react-vite": "^8.5.6",
    "@storybook/test": "^8.5.6",
    "@tanstack/react-query-devtools": "^5.66.3",
    "@tanstack/react-table-devtools": "^8.21.2",
    "@tanstack/router-devtools": "^1.105.0",
    "@tanstack/router-plugin": "^1.105.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "2.0.5",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prop-types": "^15.8.1",
    "shx": "^0.3.4",
    "storybook": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "5.4.2",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "2.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
